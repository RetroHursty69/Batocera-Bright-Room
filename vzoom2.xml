<theme>
   <formatVersion>7</formatVersion>

      <view name="system">
         <carousel name="systemcarousel">
          <zIndex>400</zIndex>	      
          <type>horizontal</type>
          <pos>0 0.38</pos>
          <size>1 1</size>
          <logoSize>0.18 0.3</logoSize>
          <color>00000000</color>
          <maxLogoCount>5</maxLogoCount>
          <logoAlignment>center</logoAlignment>                        
         </carousel>
         
		<image name="logo">
		<linearSmooth>true</linearSmooth>
			<storyboard event="activate">
				<animation property="y" to="0.5" begin="3500" duration="1000" mode="EaseIn"/>
				<animation property="x" to="0.2" begin="3500" duration="1000" mode="EaseIn"/>				
				<animation property="opacity" from="0" to="1" duration="150" mode="Linear"/>
				<animation property="opacity" from="1" to="1" begin="1150" duration="35" mode="Linear"/>
			</storyboard>
			<storyboard event="scroll">
				<animation property="y" to="0.5" begin="3500" duration="1000" mode="EaseIn"/>
				<animation property="x" to="0.2" begin="3500" duration="1000" mode="EaseIn"/>	
				<animation property="opacity" from="0" to="1" duration="150" mode="Linear"/>
				<animation property="opacity" from="1" to="1" begin="1150" duration="35" mode="Linear"/>
			</storyboard>
			<storyboard event="deactivate">
				<animation property="y" to="0.5" begin="3500" duration="1000" mode="EaseIn"/>
				<animation property="x" to="0.2" begin="3500" duration="1000" mode="EaseIn"/>	
				<animation property="opacity" from="0" to="1" duration="150" mode="Linear"/>
				<animation property="opacity" from="1" to="1" begin="1150" duration="35" mode="Linear"/>
			</storyboard>
		</image> 
		         
